{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "app.t28.dev",
  "description": "T28 のアプリ",
  "display": "minimal-ui",
  "start_url": "/?pwa=true",
  "scope": "/",
  "icons": [
    {
      "src": "/dairi@192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/dairi@512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/",
    "params": {
      "title": "web-share:title",
      "text": "web-share:text",
      "url": "web-share:url"
    }
  }
}
